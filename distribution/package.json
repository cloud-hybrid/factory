{
    "name": "cloud-factory",
    "private": false,
    "version": "0.3.56",
    "type": "module",
    "main": "index.js",
    "description": "A CLI Utility to Manage Software-Defined State",
    "types": "distribution",
    "repository": "git+https://github.com/cloud-hybrid/factory.git",
    "license": "BSD 1-Clause License",
    "author": "Jacob B. Sanders",
    "scripts": {
        "start": "node --experimental-specifier-resolution node index.js",
        "build": "tsc --pretty",
        "watch": "tsc --watch",
        "upload": "npm publish --access public",
        "reinstall": "npm update && npm install --global --force",
        "prepublishOnly": "npm install",
        "constructs": "cdktf get --language typescript --output .constructs",
        "upgrade": "npm i cdktf@latest cdktf-cli@latest",
        "upgrade-next": "npm i cdktf@next cdktf-cli@next"
    },
    "dependencies": {
        "@aws-sdk/client-s3": "^3.45.0",
        "@aws-sdk/credential-providers": "^3.45.0",
        "@aws-sdk/node-config-provider": "^3.40.0",
        "@aws-sdk/client-secrets-manager": "^3.45.0",
        "ajv": "^8.8.2",
        "ajv-formats": "^3.0.0-rc.0",
        "cdktf": "^0.8.4",
        "cdktf-cli": "^0.8.4",
        "chalk": "^5.0.0",
        "constructs": "^10.0.12",
        "uuid": "^8.3.2",
        "yargs": "^17.3.0"
    },
    "devDependencies": {
        "@aws-sdk/types": "^3.40.0",
        "@types/uuid": "^8.3.4",
        "@types/node": "^17.0.8",
        "@types/aws-lambda": "^8.10.89",
        "@types/yargs": "^17.0.8",
        "@cdktf/provider-aws": "^3.0.1",
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/experimental-utils": "^5.7.0",
        "@typescript-eslint/parser": "^5.8.0",
        "@typescript-eslint/scope-manager": "^5.7.0",
        "@typescript-eslint/typescript-estree": "^5.7.0",
        "typescript": "latest",
        "typescript-eslint-language-service": "^5.0.0"
    },
    "exports": {
        ".": "./index.js"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "rules": {
            "no-case-declarations": 0,
            "@typescript-eslint/no-explicit-any": 0,
            "@typescript-eslint/explicit-function-return-type": 0,
            "@typescript-eslint/no-use-before-define": 0,
            "@typescript-eslint/no-empty-interface": 0,
            "@typescript-eslint/explicit-module-boundary-types": 0,
            "@typescript-eslint/no-empty-function": 0,
            "@typescript-eslint/no-unused-vars": 0,
            "@typescript-eslint/no-inferrable-types": 0,
            "@typescript-eslint/ban-types": 0,
            "@typescript-eslint/ban-ts-comment": 0,
            "no-sequences": "error"
        },
        "ignorePatterns": [
            "ci",
            "dist",
            "coverage",
            "distribution",
            "node_modules"
        ]
    },
    "bin": {
        "cloud-factory": "./index.js"
    }
}
