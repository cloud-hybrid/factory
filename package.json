{
    "name": "@cloud-vault/factory",
    "private": false,
    "version": "0.0.0",
    "type": "module",
    "main": "Entry-Point.js",
    "bin": {
        "factory": "./bin/Entry-Point.js"
    },
    "files": [
        "**/*.js"
    ],
    "scripts": {
        "rm-locks": "find . -type f -name \"package-lock.json\" -prune -exec rm -rf '{}' +",
        "rm-node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
    },
    "dependencies": {
        "@cloud-vault/cdfk": "file:./cdfk",
        "@cloud-vault/cli": "file:./cli",
        "@cloud-vault/cli-prompt": "file:./prompt"
    }
}
