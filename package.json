{
    "name": "cloud-factory",
    "private": false,
    "version": "0.3.41",
    "type": "module",
    "main": "index.js",
    "description": "A CLI Utility to Manage Software-Defined State",
    "types": "distribution",
    "repository": "git+https://github.com/cloud-hybrid/factory.git",
    "license": "BSD 1-Clause License",
    "author": "Jacob B. Sanders",
    "engines": {
        "node": "^16.x"
    },
    "scripts": {
        "start": "node index.js",
        "install": "npm run build",
        "build": "[[ -f /usr/local/bin/tsc ]] && /usr/local/bin/tsc || echo \"TSC Not Found\"",
        "build-layer": "node build-layer.js",
        "watch": "/usr/local/bin/tsc --watch",
        "constructs": "cdktf get --language typescript --output .constructs",
        "upload": "npm publish --access public",
        "reinstall": "npm update && npm install --global --force",
        "prepublishOnly": "npm install",
        "rm-distribution": "rm -r -f ./distribution",
        "rm-js": "find ./src -type f -name \"*.js\" -prune -exec rm -rf '{}' +",
        "rm-locks": "find . -type f -name \"package-lock.json\" -prune -exec rm -rf '{}' +",
        "rm-types": "find ./src -type f -name \"*.d.ts\" -prune -exec rm -rf '{}' +",
        "rm-node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +",
        "upgrade": "npm i cdktf@latest cdktf-cli@latest",
        "upgrade-next": "npm i cdktf@next cdktf-cli@next"
    },
    "peerDependencies": {
        "typescript": "latest"
    },
    "dependencies": {
        "@cdktf/provider-aws": "^3.0.1",
        "@types/aws-lambda": "^8.10.89",
        "@types/node": "^17.0.8",
        "@types/yargs": "^17.0.8",
        "ajv": "^8.8.2",
        "ajv-formats": "^3.0.0-rc.0",
        "cdktf": "^0.8.4",
        "cdktf-cli": "^0.8.4",
        "chalk": "^5.0.0",
        "constructs": "^10.0.12",
        "ts-node": "^10.4.0",
        "typescript": "latest",
        "yargs": "^17.3.0"
    },
    "devDependencies": {
        "@typescript-eslint/eslint-plugin": "^5.7.0",
        "@typescript-eslint/experimental-utils": "^5.7.0",
        "@typescript-eslint/parser": "^5.8.0",
        "@typescript-eslint/scope-manager": "^5.7.0",
        "@typescript-eslint/typescript-estree": "^5.7.0",
        "cloud-factory": ".",
        "regenerator-runtime": "^0.13.9",
        "typescript-eslint-language-service": "^5.0.0"
    },
    "exports": {
        ".": "./index.js"
    },
    "eslintConfig": {
        "parser": "@typescript-eslint/parser",
        "plugins": [
            "@typescript-eslint"
        ],
        "extends": [
            "eslint:recommended",
            "plugin:@typescript-eslint/eslint-recommended",
            "plugin:@typescript-eslint/recommended"
        ],
        "rules": {
            "no-case-declarations": 0,
            "@typescript-eslint/no-explicit-any": 0,
            "@typescript-eslint/explicit-function-return-type": 0,
            "@typescript-eslint/no-use-before-define": 0,
            "@typescript-eslint/no-empty-interface": 0,
            "@typescript-eslint/explicit-module-boundary-types": 0,
            "@typescript-eslint/no-empty-function": 0,
            "@typescript-eslint/no-unused-vars": 0,
            "@typescript-eslint/no-inferrable-types": 0,
            "@typescript-eslint/ban-types": 0,
            "@typescript-eslint/ban-ts-comment": 0,
            "no-sequences": "error"
        },
        "ignorePatterns": [
            "ci",
            "dist",
            "coverage",
            "distribution",
            "node_modules"
        ]
    },
    "bin": {
        "cloud-factory": "./index.js"
    }
}
