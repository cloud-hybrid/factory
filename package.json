{
    "name": "@cloud-vault/factory",
    "private": false,
    "version": "0.0.0",
    "type": "module",
    "bin": {
        "factory": "./cli/index.js"
    },
    "files": [
        "**/*.js",
        "package.json"
    ],
    "scripts": {
        "rm-locks": "find . -type f -name \"package-lock.json\" -prune -exec rm -rf '{}' +",
        "rm-node-modules": "find . -name 'node_modules' -type d -prune -exec rm -rf '{}' +"
    },
    "dependencies": {
        "@cloud-vault/factory": ".",
        "@cloud-vault/cdfk": "./cdfk",
        "@cloud-vault/cli": "./cli",
        "@cloud-vault/cli-prompt": "./prompt"
    }
}
